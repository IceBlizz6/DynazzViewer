{
  "name": "dynazzviewer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "vite build"
  },
  "dependencies": {
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vue-class-component": "^8.0.0-rc.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "typescript": "~4.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "vue/html-indent": [
        "error",
        "tab"
      ],
      "@typescript-eslint/indent": [
        "error",
        "tab"
      ],
      "@typescript-eslint/no-non-null-assertion": "error",
      "@typescript-eslint/explicit-function-return-type": [
        "error"
      ],
      "@typescript-eslint/member-delimiter-style": [
        "warn",
        {
          "multiline": {
            "delimiter": "none"
          },
          "singleline": {
            "delimiter": "comma"
          }
        }
      ],
      "@typescript-eslint/explicit-member-accessibility": [
        "warn"
      ],
      "@typescript-eslint/semi": [
        "warn",
        "never"
      ],
      "@typescript-eslint/no-inferrable-types": "off",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  }
}
